@section scripts{
    <script type="text/javascript">

        var offcanvas = new KTOffcanvas('kt_user_profile_aside', {
            overlay: true,
            baseClass: 'kt-app__aside',
            closeBy: 'kt_user_profile_aside_close',
            toggleBy: 'kt_subheader_mobile_toggle'
        });

        var avatar = new KTAvatar('kt_user_avatar');

        $(".kt-widget__item").click(function () {

            $(".kt-widget__item--active").removeClass("kt-widget__item--active");

            var target = $(this).attr("href");

            if ($(target).length > 0) {
                $(this).addClass("kt-widget__item--active");
                $(".kt-app__content:visible").hide();
                $(target).show();
            }

        })

    </script>
}