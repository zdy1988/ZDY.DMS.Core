@section Scripts{
    <script type="text/javascript">
        var id = @Model.UserId

        require([
            "jquery",
            "ko",
            "zdy",
            "bootstrap",
            "jquery-slimscroll",
            "bootstrap-citypicker",
            "bootstrap-fileinput",
            ui], function ($, ko, zdy) {

                var ui = arguments[arguments.length - 1];
                ui.init();

                var form = zdy.form().options({
                    fields: [
                        { field: 'Id', value: ko.observable(0) },
                        { field: 'Avatar', value: ko.observable() },
                        { field: 'AvatarUrl', value: ko.observable() },
                        { field: 'DepartmentId', value: ko.observable() },
                        { field: 'Email', value: ko.observable() },
                        { field: 'Gender', value: ko.observable() },
                        { field: 'IsDisabled', value: ko.observable() },
                        { field: 'Mobile', value: ko.observable() },
                        { field: 'NickName', value: ko.observable() },
                        { field: 'Phone', value: ko.observableArray() },
                        { field: 'Position', value: ko.observable() },
                        { field: 'UserName', value: ko.observable() },
                        { field: 'City', value: ko.observable() },
                        { field: 'Country', value: ko.observable("China") },
                        { field: 'Province', value: ko.observable() }
                    ],
                    dataUpdateUrl: 'User/Update'
                }).import(function () {

                    this.activePage = ko.observable(1)

                    this.setActivePage = function (data, e) { 
                        var index = $(e.target).data("index")
                        this.activePage(index)
                    }

                    this.afterLoad = function (data) {

                        $("#city-picker").citypicker({
                            responsive: true,
                            level: "city"
                        });

                        $("#city-picker").on("cp:updated", function () {
                            var city = $('#city-picker').val()
                            if (city !== undefined) {
                                var address = city.split("/");
                                var city = address[0] === undefined ? "" : address[0]
                                var province = address[1] === undefined ? "" : address[1]
                                form.City(city)
                                form.Province(province)
                            }
                        })
                    }

                    this.Citys = ko.computed(function () {
                        var array = [this.City(), this.Province()]
                        return array.join("/")
                    }, this)

                    this.OrgPassword = ko.observable()
                    this.NewPassword = ko.observable()
                    this.NewPassword2 = ko.observable()

                    this.changePassword = function () {
                        var op = this.OrgPassword()
                        var np = this.NewPassword()
                        var np2 = this.NewPassword2()
                        if (op.length > 0 && np.length > 0 && np2.length > 0) {
                            if (np == np2) {
                                zdy.ajaxPost("TokenAuth/ChangePassword", { orgPassword: op, newPassword: np }).done(function () {
                                    zdy.toastr.success("修改成功")
                                    form.resetPassword()
                                })
                            } else {
                                zdy.toastr.error("两次输入的新密码不一致")
                            }
                        } else {
                            zdy.toastr.error("请将新旧密码填写完整")
                        }
                    }
                    this.resetPassword = function () {
                        this.OrgPassword("")
                        this.NewPassword("")
                        this.NewPassword2("")
                    }

                    this.handleUploadAvatar = function () {
                        var input = document.getElementById("avatarUploader")
                        zdy.upload({ type: "Avatar", input: input }).done(function (rst) {
                            var files = rst.Data
                            form.Avatar(files[0].Id)
                            form.AvatarUrl(files[0].Url)
                            form.update()
                        })
                    }


                }).bind("profile").load("User/FindUserById", { id: id });

            })
    </script>
}